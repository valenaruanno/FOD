        nomYape: str30;
        dni: longint;
        legajo: integer;
        anioI: str8;
    end;
    
    nodo = record
        cant_datos: integer;
        datos: array[1..M-1] of alu;
        hijos: array[1..M] of integer;
    end;
    
    arbolB: file of nodo;
    

var
    archivoIndice: arbolB;

